{
  "name": "storybook-addon-tailwind-autodocs",
  "version": "1.0.3",
  "description": "Design system documentation that comes directly from your tailwind config.",
  "keywords": [
    "tailwind",
    "css",
    "layout",
    "appearance",
    "style",
    "design",
    "system",
    "auto",
    "docs",
    "storybook-addons"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/matanio/storybook-addon-tailwind-autodocs"
  },
  "type": "module",
  "license": "MIT",
  "author": "Matan Yosef",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preset": "./dist/preset.cjs",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "npm run build -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "run-p build:watch \"storybook --quiet\"",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "npm run build && auto shipit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@frontline-hq/recma-sections": "^1.0.8",
    "@storybook/addon-essentials": "^8.2.7",
    "@storybook/addon-interactions": "^8.2.7",
    "@storybook/addon-links": "^8.2.7",
    "@storybook/blocks": "^8.2.7",
    "@storybook/components": "^8.2.7",
    "@storybook/core-common": "^8.2.7",
    "@storybook/core-events": "^8.2.7",
    "@storybook/csf-tools": "^8.2.7",
    "@storybook/manager": "^8.2.7",
    "@storybook/manager-api": "^8.2.7",
    "@storybook/mdx2-csf": "^1.1.0",
    "@storybook/preview": "^8.2.7",
    "@storybook/preview-api": "^8.2.7",
    "@storybook/react": "^8.2.7",
    "@storybook/react-vite": "^8.2.7",
    "@storybook/test": "^8.2.7",
    "@storybook/theming": "^8.2.7",
    "@storybook/types": "^8.2.7",
    "@types/node": "^18.15.0",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "auto": "^11.1.1",
    "autoprefixer": "^10.4.19",
    "boxen": "^7.1.1",
    "dedent": "^1.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.40",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^8.2.7",
    "tailwindcss": "^3.4.7",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "unplugin": "^1.12.0",
    "vite": "^5.3.5",
    "zx": "^7.2.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "exportEntries": [
      "src/addon/index.ts"
    ],
    "nodeEntries": [
      "src/addon/preset.ts"
    ]
  },
  "storybook": {
    "displayName": "Tailwind Autodocs",
    "supportedFrameworks": [
      "react",
      "vue"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
